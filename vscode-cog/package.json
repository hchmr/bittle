{
    "name": "vscode-cog",
    "displayName": "Cog",
    "description": "Cog language support for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "anon",
    "engines": {
        "vscode": "^1.91.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cog",
                "aliases": [
                    "Cog",
                    "cog"
                ],
                "extensions": [
                    ".cog",
                    ".cogh"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cog",
                "scopeName": "source.cog",
                "path": "./cog.tmLanguage.json"
            }
        ]
    }
}