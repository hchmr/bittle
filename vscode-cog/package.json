{
    "name": "vscode-cog",
    "publisher": "anon",
    "version": "0.0.1",
    "displayName": "Cog",
    "description": "Language support for Cog",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "grammars": [
            {
                "language": "cog",
                "scopeName": "source.cog",
                "path": "./cog.tmLanguage.json"
            }
        ],
        "languages": [
            {
                "id": "cog",
                "aliases": [
                    "Cog"
                ],
                "extensions": [
                    ".cog",
                    ".cogs"
                ],
                "configuration": "./language-configuration.json"
            }
        ]
    },
    "activationEvents": [
        "*.cog"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "prepare": "lezer-generator src/cog.grammar --typeScript --output src/parser.ts",
        "compile": "npm run prepare && tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@lezer/lr": "^1.4.0"
    },
    "devDependencies": {
        "@types/node": "^20.12.11",
        "@types/vscode": "^1.89.0",
        "typescript": "^5.4.5",
        "@lezer/generator": "^1.7.0"
    }
}